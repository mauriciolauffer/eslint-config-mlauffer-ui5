{
  "name": "eslint-config-mlauffer-ui5",
  "version": "1.0.2",
  "description": "ESLint shareable config for UI5",
  "private": false,
  "author": "Mauricio Lauffer",
  "license": "MIT",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint lib --cache",
    "lint:ci": "eslint lib -f @microsoft/eslint-formatter-sarif -o eslint.sarif",
    "format": "prettier . --write --cache && npm run lint -- --fix",
    "test": "vitest run"
  },
  "keywords": [
    "eslint",
    "config",
    "javascript",
    "ui5",
    "openui5",
    "sapui5"
  ],
  "dependencies": {
    "@eslint/js": "^9.10.0",
    "eslint-config-mlauffer-nodejs": "^2.0.5",
    "eslint-plugin-no-unsanitized": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.5.5",
    "prettier": "^3.3.3",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18"
  },
  "cpu": [
    "arm64",
    "x64"
  ],
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mauriciolauffer/eslint-config-mlauffer-ui5.git"
  },
  "bugs": {
    "url": "https://github.com/mauriciolauffer/eslint-config-mlauffer-ui5.git/issues"
  }
}
